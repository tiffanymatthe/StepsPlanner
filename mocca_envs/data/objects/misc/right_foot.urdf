<?xml version="1.0"?>
<robot name="foot">

	<!-- * * * Link Definitions * * * -->

 	<link name="base_link">
		<visual>
		    <origin xyz="0 0 0.0" rpy="0 0 0"/>
			<geometry>
				<box size="1.5 1 0.02"/>
			</geometry>
		</visual>

    <!-- Add five small boxes -->
        <visual name="small_boxes" >
            <origin xyz="1.2 0.3 0.0" rpy="0 0 0"/>
            <geometry>
                <box size="1 0.5 0.02"/>
            </geometry>
        </visual>
    </link>
  
</robot>